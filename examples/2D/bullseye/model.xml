<?xml version="1.0"?>
<model>

    <model-type>AccelerationRegression</model-type>
    <dimension>2</dimension>

    <template>
        <object id="img">
            <deformable-object-type>Image</deformable-object-type>
            <filename>bullseye1_blur.png</filename>
            <attachment-type>l2</attachment-type>
            <data-weight>10</data-weight>
        </object>
    </template>

    <deformation-parameters>
        <number-of-timepoints>30</number-of-timepoints>
        <kernel-width>10</kernel-width>
        <kernel-type>torch</kernel-type>
        <tmin>0</tmin>
        <tmax>4</tmax>
        <regularity-weight>0.001</regularity-weight>
        <initial-velocity-weight>1</initial-velocity-weight>
    </deformation-parameters>

</model>
